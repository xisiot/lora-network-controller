{
  "database": {
    "mysql": {
      "username": "username",
      "password": "password",
      "database": "mysql",
      "host": "localhost",
      "port": 3306,
      "dialect": "mysql",
      "operatorsAliases": false,
      "logging": false,
      "timezone": "+08:00",
      "define": {
        "freezeTableName": true,
        "timestamp": true,
        "charset": "utf8"
      },
      "pool": {
        "max": 10,
        "min": 1,
        "idle": 10000,
        "acquire": 30000
      }
    },
    "redis": {
      "cluster": false,
      "options": [
        {
          "host": "localhost",
          "port": 6379
        }
      ]
    }
  },
  "mocha": {
    "timeout": 5000,
    "longTimeOut": 15000
  },
  "log": {
    "level": "debug",
    "colorize": true
  },
  "mqClient_nct": {
    "consumerGroup": {
      "options": {
        "kafkaHost": "localhost:9092",
        "groupId": "lora-network-controller-message-dispatch-in",
        "sessionTimeout": 15000,
        "protocol": [
          "roundrobin"
        ],
        "fromOffset": "latest"
      },
      "topics": [
        "CS-sub"
      ]
    },
    "client": {
      "kafkaHost": "localhost:9092",
      "clientId": "lora-network-controller-message-dispatch-out"
    },
    "producer": {
      "requireAcks": 1,
      "ackTimeoutMs": 100,
      "partitionerType": 2
    },
    "schemaPath": {
      "messages": "config/messages.json",
      "common": "config/common.json"
    },
    "topics": {
      "pubToServer": "CS-pub",
      "subFromServer": "CS-sub"
    }
  }
}